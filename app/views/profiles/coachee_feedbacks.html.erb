<div class="page-wrapper">
  <div class="grid">

    <div class="row">

    <% @feedbacks.each do |feedback| %>
      <div class="row">
        <div class="col-xs-12 col-md-6 col-md-offset-3">
          <div class="card-wrapper">
              <div class="card-title">
               <%= "#{feedback.workout.date.strftime("%d.%b.%y")}  - #{feedback.exercice.name}" %>
              </div>
              <div class="card-content">
                <%= render "workoutsets/feedback", workoutset: feedback %>
              </div>
          </div>
        </div>
      </div>
    <% end %>


  </div>
</div>

