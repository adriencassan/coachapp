<div class="page-wrapper">
  <div class="grid">

    <div class="row">
      <div class="col-xs-12 col-md-6 col-md-offset-3">
        <% @coachees.each do |coachee| %>
          <div class="card-wrapper">
            <div class="card-content align-vertical height">
              <div class="row">
                <div class="col-xs-8"><%= coachee.first_name %></div>
                <div class="col-xs-4 custom-table-cell">
                  <%= link_to profile_profile_coachee_path(coachee), class:"btn-icon" do %><i class="fas fa-chevron-circle-right" style= "font-size: 15px"></i><%end%>
                </div>
              </div>
            </div>
          </div>
        <% end %>
      </div>
    </div>
  </div>
</div>


